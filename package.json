{
  "name": "chronoskit",
  "version": "0.0.0-beta.3",
  "description": "Localized date formatter for Persian and Gregorian calendars.",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc && tsc -p tsconfig.json && tsc -p tsconfig.cjs.json",
    "prepare": "npm run build"
  },
  "keywords": [
    "intl",
    "date",
    "calendar",
    "persian",
    "typescript",
    "date",
    "date convert",
    "persian date",
    "gregorian date",
    "persian calendar",
    "gregorian calendar"
  ],
  "author": "Hadi",
  "license": "MIT"
}
